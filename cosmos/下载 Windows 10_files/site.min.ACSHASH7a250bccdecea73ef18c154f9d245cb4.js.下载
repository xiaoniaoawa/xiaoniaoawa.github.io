'use strict';$(function(){function b(a){(a=$(a))&&(a.hasClass("collapsed")?a.attr("data-bi-bhvr",7):a.attr("data-bi-bhvr",8))}$(".accordion li .accordion-header .btn-collapse").each(function(){b(this)});$(".accordion-header .btn-collapse").on("click",function(){b(this)});document.querySelectorAll(".accordion sup").forEach(function(a){0==a.children.length&&a.insertAdjacentHTML("afterbegin",'\x3cspan class\x3d"sr-text"\x3eFootnote\x3c/span\x3e')})});